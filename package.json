{
  "name": "@hypi/hypi",
  "description": "Application developer community centred around Hypi's unique low-code, serverless development platform.",
  "version": "1.0.0",
  "author": "Hypi @https://hypi.io/",
  "bin": {
    "hypi": "bin/run"
  },
  "bugs": {
    "url": "https://github.com/hypi-universe/hypi-cli"
  },
  "dependencies": {
    "@apollo/client": "^3.3.12",
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-commands": "^1.3.0",
    "@oclif/plugin-help": "^3.2.2",
    "@oclif/plugin-update": "^1.3.10",
    "@oclif/plugin-warn-if-update-available": "^1.7.0",
    "@types/fs-extra": "^9.0.11",
    "atob": "^2.1.2",
    "await-timeout": "^1.1.1",
    "axios": "^0.21.1",
    "conf-cli": "^0.1.9",
    "cross-fetch": "^3.1.2",
    "exec-sh": "^0.4.0",
    "graphql": "^15.5.0",
    "inquirer": "^6.5.2",
    "json-multiline-strings": "^0.1.0",
    "listr": "^0.14.3",
    "shelljs": "^0.8.4",
    "tslib": "^1.14.1",
    "yaml": "^2.0.0-4"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "@types/atob": "^2.1.2",
    "@types/await-timeout": "^0.3.1",
    "@types/chai": "^4.2.15",
    "@types/execa": "^2.0.0",
    "@types/inquirer": "^7.3.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^14.14.41",
    "chai": "^4.3.4",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.9"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/hypi-universe/hypi-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "dirname": "hypi",
    "commands": "./lib/commands",
    "bin": "hypi",
    "plugins": [
      "@oclif/plugin-help",
      "conf-cli",
      "@oclif/plugin-commands",
      "@oclif/plugin-update",
      "@oclif/plugin-warn-if-update-available"
    ],
    "hooks": {
      "hypi-config": "./lib/hooks/hypi/set-global-config"
    },
    "macos": {
      "identifier": "com.hypi.cli"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hypi-universe/hypi-cli.git"
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "test"
  }
}
