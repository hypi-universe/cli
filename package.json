{
  "name": "hypi",
  "description": "hypi command line interface",
  "version": "0.0.0",
  "author": "Eman @engeman2008",
  "bin": {
    "hypi": "./bin/run"
  },
  "bugs": "https://github.com/engeman2008/hypi/issues",
  "dependencies": {
    "@apollo/client": "^3.3.12",
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "axios": "^0.21.1",
    "conf-cli": "^0.1.9",
    "cross-fetch": "^3.1.2",
    "graphql": "^15.5.0",
    "json-multiline-strings": "^0.1.0",
    "tslib": "^1.14.1",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "@types/chai": "^4.2.15",
    "@types/fs-extra": "^9.0.8",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.55",
    "chai": "^4.3.4",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.9"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/engeman2008/hypi",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "hypi",
    "plugins": [
      "@oclif/plugin-help",
      "conf-cli"
    ],
    "hooks": {
      "hypi-config": "./lib/hooks/hypi/set-global-config"
    }
  },
  "repository": "engeman2008/hypi",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
